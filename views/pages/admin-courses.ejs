<!doctype html>
<html lang="en">
<head>
	<title>Courses | Admin | Codeshala | Get Better At Programing By Joining The Best programing courses in Delhi NCR</title> 
	<% include ../partials/header %>
	<% include ../partials/metadata %>
</head>
<body>
	<% include ../partials/nav %>
	<div class="container">
		<div class="row">
			<div class="col-lg-3">
				<% include ../partials/admin-menu %>
			</div>
			<div class="col-lg-9">
				<nav aria-label="breadcrumb">
					<ol class="breadcrumb">
						<li class="breadcrumb-item"><a href="/admin">Admin Home</a></li>
						<li class="breadcrumb-item"><a href="/admin/courses">View All Courses</a></li>
					</ol>
				</nav>
				<br/>
				<div class="card">
					<div class="card-header">
						View All Courses
					</div>
					<div class="card-body">
						<div class="row">
							<% for(key in courses){ %>
							<div class="card border-info lg-6" style="max-width: 18rem;margin:10px;">
								<div class="card-header"><%= courses[key].attrs.name %></div>
								<div class="card-body text-info">
									<% if (courses[key].attrs.courseRegistrationStatus=="OPEN") { %>
									<span>Open</span>
									<% } else { %>
									<span>Open</span>
									<% } %> | 
									<%= courses[key].attrs.minsessions %> - <%= courses[key].attrs.maxsessions %> Sessions<br/>
									
								</div>
							</div>
							<% } %>

							
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<% include ../partials/footer %>
</body>

<% include ../partials/footer-scripts %>

</html>