	<% include ../partials/nav %>
	<div class="container text-center codeshala-heading">
		<h1><span style="color:#fd8900">CODE</span><span style="color:#0289fe">SHALA</span><br/>GET BETTER AT PROGRAMING</h1>
		<h2>
			We are community of programmers. Our community is lead by professionals who have broad experience in competitve programming and software development. Our community mentors are working at top companies and providing mentorship to candidates across different colleges. We provide learning courses to make you a better programmer.
		</h2>
		<span class="text-center codeshala-theme codeshala-course-card-btn">
			<a href="/tools/course-chooser">
				<small>Not able to decide which course to join?</small>
				<br/>Try our course chooser  <i class="fa fa-arrow-right"></i>
			</a>
		</span>
	</div>
	<hr>
	<div class="container text-center" id="courses-section">
		<div class="row mx-auto">
			<% for(key in courses){ %>
			<div class="card col-lg-3 codeshala-course-card">
				<% if (courses[key].attrs.courseRegistrationStatus=="OPEN") { %>
				<div class="ribbon ribbon-top-left"><span>Open</span></div>
				<% } else { %>
				<div class="ribbon ribbon-top-left"><span>Open</span></div>
				<% } %>
				<div class="codeshala-course-thumbnail" style="background-image:url(assets/images/course-icon/algo.png);background-size:cover;background-repeat: no-repeat;"></div>
				<h2><a href="/course/<%= courses[key].attrs.alias %>"><%= courses[key].attrs.name %></a></h2>
				<p><%= courses[key].attrs.minsessions %> - <%= courses[key].attrs.maxsessions %> Sessions<br/><%= courses[key].attrs.description %></p>
				<span class="text-center codeshala-theme codeshala-course-card-btn"><a href="/course/<%= courses[key].attrs.alias %>">Explore more &amp; register <i class="fa fa-arrow-right"></i></a></span>
			</div>
			<% } %>
			<!-- Insert new course above this line-->
		</div>
	</div>
